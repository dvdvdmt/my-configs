<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
        "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist>
    <dict>
        <key>Label</key>
        <string>local.restart_keyboard_maestro</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/bash</string>
            <string>-c</string>
            <string>osascript ~/Library/LaunchAgents/local.restart_keyboard_maestro.scpt</string>
        </array>
        <key>RunAtLoad</key>
        <false/>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>5</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
        <key>StandardErrorPath</key>
        <string>/tmp/restart_keyboard_maestro.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/restart_keyboard_maestro.out</string>
    </dict>
</plist>
