<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
        "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist>
    <dict>
        <key>Label</key>
        <string>local.open_book</string>
        <key>ProgramArguments</key>
        <array>
            <string>/Applications/calibre.app/Contents/MacOS/ebook-viewer</string>
            <string>--continue</string>
        </array>
        <key>RunAtLoad</key>
        <false/>
        <key>StartCalendarInterval</key>
        <dict>
            <key>Hour</key>
            <integer>5</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
        <key>StandardErrorPath</key>
        <string>/tmp/open_book.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/open_book.out</string>
    </dict>
</plist>
